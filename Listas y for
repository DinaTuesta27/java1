        //INTENTO DOS.
        Scanner objeto=new Scanner (System.in);
        System.out.println("Digite cantidad de veces: ");
        int veces=objeto.nextInt();
        int arreglo[]=new int[veces];
        for(int i=0;i<veces;i++){
            System.out.println("Ingrese valores: ");
            int k=objeto.nextInt();
            arreglo[i]=k;
        }
        for(int i=0;i<arreglo.length;i++){
            System.out.println("El resultado es: "+arreglo[i]);  
        }
        
    }
    
}
